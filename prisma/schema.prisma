generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model SupportedLeague {
  id        Int    @id @default(autoincrement())
  name      String @unique
  shortName String @default("")
  logo      String @default("")
}

model User {
  id       String      @id
  email    String
  provider String
  name     String
}
